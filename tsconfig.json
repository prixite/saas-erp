{
  "compilerOptions": {
    "lib": ["es6", "dom"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@src/*": ["frontend/src/*"]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
  },
  "include": [
    "frontend/src/**/*"
  ]
}
